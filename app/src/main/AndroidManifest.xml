<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.google_maps">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Google_Maps">
        <activity
            android:name=".VideoAct"
            android:exported="true"
            android:label="@string/title_activity_video"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".Authentication"
            android:exported="true"
            android:label="@string/title_activity_authentication"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".LazyVerticalGrid"
            android:exported="true"
            android:label="@string/title_activity_lazy_vertical_grid"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".PagerAct"
            android:exported="true"
            android:label="@string/title_activity_pager"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".AnimatedAct"
            android:exported="true"
            android:label="@string/title_activity_animated"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".TabAct"
            android:exported="true"
            android:label="@string/title_activity_tab"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".AdMobCompose"
            android:exported="false"
            android:label="@string/title_activity_ad_mob_compose"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".Admob"
            android:exported="false" />
        <activity
            android:name=".ButtonNavigationView"
            android:exported="false"
            android:label="@string/title_activity_intent_button_navigation_view"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".SearchView"
            android:exported="false"
            android:label="@string/title_activity_search_view"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".Gradient"
            android:exported="false"
            android:label="@string/title_activity_gradient"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".RecuclerView"
            android:exported="false"
            android:label="@string/title_activity_recucler_view"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713" />

        <activity
            android:name=".XMLMaps"
            android:exported="false" />
        <activity
            android:name=".ComposeMaps"
            android:exported="false"
            android:label="@string/title_activity_compose_maps"
            android:theme="@style/Theme.Google_Maps.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Google_Maps.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>